
#include <iostream>  //HEADER FILE
#include <bits/stdc++.h>

using namespace std;  
int main()            
{  
   int i,fact=1,number; 
   //ENTER ANY NUMBER
  cout<<"Enter any Number: ";    
 cin>>number;    
  for(i=1;i<=number;i++)     //FOR USED
  {    
      fact=fact*i;    
  }    
  cout<<"Factorial of " <<number<<" is: "<<fact<<endl;  
  return 0;  
}  
